[Desktop Entry]
Type=Application
Name=nnn
Comment=Terminal file manager
# d: details, H: hidden, e: edit mode, S presistent session
Exec=urxvtc -name nnn -e zsh -ic "nnn -dHeS %F"
# Exec=urxvtc -name nnn -e /usr/bin/nnn -dHe %f
# Exec=urxvtc -name "nnn" -e /usr/bin/nnn -ed %f
Terminal=false
Icon=nnn
MimeType=inode/directory
Categories=System;FileTools;FileManager;ConsoleOnly
Keywords=File;Manager;Management;Explorer;Launcher
StartupWMClass=NNN
